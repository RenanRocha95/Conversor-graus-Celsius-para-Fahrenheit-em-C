#include <stdio.h>

int main() {
    
float celsius, fahrenheit;

// CALCULADORA EM PORTUGOL.
// SIGA @RENANCYBROG NO INSTAGRAM
// GITHUB https://github.com/RenanRocha95/

printf("Digite qual é a temperatura em graus Celsius: ");
scanf("%f", &celsius);

fahrenheit = (celsius * 9.0 / 5.0) + 32.0;

printf("A conversão da temperatura de Celsius para Fahrenheit é: %.2f\n", fahrenheit);

return 0;
}
